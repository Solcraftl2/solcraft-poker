{
  "buildCommand": "cd sol-craft && npm run build",
  "outputDirectory": "sol-craft/.next",
  "installCommand": "cd sol-craft && npm install",
  "framework": "nextjs",
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/sol-craft/$1"
    }
  ]
}

